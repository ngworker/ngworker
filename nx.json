{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "npmScope": "ngworker",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "accessToken": "ZTViM2YyNGQtNjk2My00YmFjLWJlYmQtZjIwMzJhMmIxNGMwfHJlYWQtd3JpdGU=",
        "cacheableOperations": ["build", "e2e", "lint", "test"],
        "canTrackAnalytics": true,
        "parallel": 6,
        "showUsageWarnings": true
      }
    }
  },
  "workspaceLayout": {
    "appsDir": "e2e",
    "libsDir": "packages"
  },
  "cli": {
    "packageManager": "yarn"
  },
  "generators": {
    "@nrwl/angular:application": {
      "e2eTestRunner": "cypress",
      "linter": "eslint",
      "routing": false,
      "strict": true,
      "style": "scss",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "displayBlock": true,
      "skipTests": true,
      "style": "scss"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "strict": true,
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:storybook-configuration": {
      "linter": "eslint"
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    }
  }
}
