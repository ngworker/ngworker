{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "pnpm"
  },
  "generators": {
    "@nrwl/angular:application": {
      "e2eTestRunner": "cypress",
      "linter": "eslint",
      "routing": false,
      "strict": true,
      "style": "scss",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "displayBlock": true,
      "skipTests": true,
      "style": "scss"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "strict": true,
      "style": "scss",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:storybook-configuration": {
      "linter": "eslint"
    }
  },
  "projects": {
    "examples-tour-of-heroes-crisis-center": {
      "projectType": "library",
      "root": "packages/examples/tour-of-heroes/crisis-center",
      "sourceRoot": "packages/examples/tour-of-heroes/crisis-center/src",
      "prefix": "app-crisis",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "packages/examples/tour-of-heroes/crisis-center/tsconfig.lib.json",
            "project": "packages/examples/tour-of-heroes/crisis-center/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/examples/tour-of-heroes/crisis-center/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/examples/tour-of-heroes/crisis-center/src/**/*.ts",
              "packages/examples/tour-of-heroes/crisis-center/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/examples/tour-of-heroes/crisis-center"
          ],
          "options": {
            "jestConfig": "packages/examples/tour-of-heroes/crisis-center/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "internal-test-util": {
      "projectType": "library",
      "root": "packages/internal/test-util",
      "sourceRoot": "packages/internal/test-util/src",
      "prefix": "ngworker",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "packages/internal/test-util/tsconfig.lib.json",
            "project": "packages/internal/test-util/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/internal/test-util/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/internal/test-util/src/**/*.ts",
              "packages/internal/test-util/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/internal/test-util"],
          "options": {
            "jestConfig": "packages/internal/test-util/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "spectacular": {
      "projectType": "library",
      "root": "packages/spectacular",
      "sourceRoot": "packages/spectacular/src",
      "prefix": "spectacular",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:package",
          "options": {
            "tsConfig": "packages/spectacular/tsconfig.lib.json",
            "project": "packages/spectacular/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/spectacular/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/spectacular/src/**/*.ts",
              "packages/spectacular/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/spectacular"],
          "options": {
            "jestConfig": "packages/spectacular/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "workspace-tools": {
      "projectType": "tool",
      "root": "tools",
      "sourceRoot": "tools",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["tools/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/tools"],
          "options": {
            "jestConfig": "tools/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "material": {
      "projectType": "library",
      "root": "packages/material",
      "sourceRoot": "packages/material/src",
      "prefix": "mat",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "packages/material/tsconfig.lib.json",
            "project": "packages/material/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/material/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/material/src/**/*.ts",
              "packages/material/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/material/src/test.ts",
            "tsConfig": "packages/material/tsconfig.spec.json",
            "karmaConfig": "packages/material/karma.conf.js"
          }
        }
      }
    },
    "cdk": {
      "projectType": "library",
      "root": "packages/cdk",
      "sourceRoot": "packages/cdk/src",
      "prefix": "cdk",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "packages/cdk/tsconfig.lib.json",
            "project": "packages/cdk/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/cdk/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/cdk/src/**/*.ts",
              "packages/cdk/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/cdk/src/test.ts",
            "tsConfig": "packages/cdk/tsconfig.spec.json",
            "karmaConfig": "packages/cdk/karma.conf.js"
          }
        }
      }
    },
    "mat-table-spreadsheet": {
      "projectType": "application",
      "root": "e2e/mat-table-spreadsheet",
      "sourceRoot": "e2e/mat-table-spreadsheet/src",
      "prefix": "ngworker",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/e2e/mat-table-spreadsheet",
            "index": "e2e/mat-table-spreadsheet/src/index.html",
            "main": "e2e/mat-table-spreadsheet/src/main.ts",
            "polyfills": "e2e/mat-table-spreadsheet/src/polyfills.ts",
            "tsConfig": "e2e/mat-table-spreadsheet/tsconfig.app.json",
            "aot": true,
            "assets": [
              "e2e/mat-table-spreadsheet/src/favicon.ico",
              "e2e/mat-table-spreadsheet/src/assets"
            ],
            "styles": [
              "e2e/mat-table-spreadsheet/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "e2e/mat-table-spreadsheet/src/environments/environment.ts",
                  "with": "e2e/mat-table-spreadsheet/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "mat-table-spreadsheet:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "mat-table-spreadsheet:build:production"
            }
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mat-table-spreadsheet:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "e2e/mat-table-spreadsheet/src/**/*.ts",
              "e2e/mat-table-spreadsheet/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/e2e/mat-table-spreadsheet"
          ],
          "options": {
            "jestConfig": "e2e/mat-table-spreadsheet/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "mat-table-spreadsheet-e2e": {
      "root": "e2e/mat-table-spreadsheet-e2e",
      "sourceRoot": "e2e/mat-table-spreadsheet-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "e2e/mat-table-spreadsheet-e2e/cypress.json",
            "tsConfig": "e2e/mat-table-spreadsheet-e2e/tsconfig.e2e.json",
            "devServerTarget": "mat-table-spreadsheet:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mat-table-spreadsheet:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "e2e/mat-table-spreadsheet-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  }
}
