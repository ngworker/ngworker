{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "pnpm"
  },
  "generators": {
    "@nrwl/angular:application": {
      "e2eTestRunner": "cypress",
      "linter": "eslint",
      "routing": false,
      "strict": true,
      "style": "scss",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "displayBlock": true,
      "skipTests": true,
      "style": "scss"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "strict": true,
      "style": "scss",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:storybook-configuration": {
      "linter": "eslint"
    }
  },
  "projects": {
    "examples-tour-of-heroes-crisis-center": {
      "projectType": "library",
      "root": "packages/examples/tour-of-heroes/crisis-center",
      "sourceRoot": "packages/examples/tour-of-heroes/crisis-center/src",
      "prefix": "app-crisis",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "packages/examples/tour-of-heroes/crisis-center/tsconfig.lib.json",
            "project": "packages/examples/tour-of-heroes/crisis-center/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/examples/tour-of-heroes/crisis-center/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/examples/tour-of-heroes/crisis-center/src/**/*.ts",
              "packages/examples/tour-of-heroes/crisis-center/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/examples/tour-of-heroes/crisis-center"
          ],
          "options": {
            "jestConfig": "packages/examples/tour-of-heroes/crisis-center/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "internal-test-util": {
      "projectType": "library",
      "root": "packages/internal/test-util",
      "sourceRoot": "packages/internal/test-util/src",
      "prefix": "ngworker",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "packages/internal/test-util/tsconfig.lib.json",
            "project": "packages/internal/test-util/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/internal/test-util/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/internal/test-util/src/**/*.ts",
              "packages/internal/test-util/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/internal/test-util"
          ],
          "options": {
            "jestConfig": "packages/internal/test-util/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "spectacular": {
      "projectType": "library",
      "root": "packages/spectacular",
      "sourceRoot": "packages/spectacular/src",
      "prefix": "spectacular",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:package",
          "options": {
            "tsConfig": "packages/spectacular/tsconfig.lib.json",
            "project": "packages/spectacular/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/spectacular/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/spectacular/src/**/*.ts",
              "packages/spectacular/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/spectacular"
          ],
          "options": {
            "jestConfig": "packages/spectacular/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "workspace-tools": {
      "projectType": "tool",
      "root": "tools",
      "sourceRoot": "tools",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "tools/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/tools"
          ],
          "options": {
            "jestConfig": "tools/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "schematics": {
      "root": "packages/schematics",
      "sourceRoot": "packages/schematics/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/schematics/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/schematics"
          ],
          "options": {
            "jestConfig": "packages/schematics/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/packages/schematics",
            "tsConfig": "packages/schematics/tsconfig.lib.json",
            "packageJson": "packages/schematics/package.json",
            "main": "packages/schematics/src/index.ts",
            "assets": [
              "packages/schematics/*.md",
              {
                "input": "./packages/schematics/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/schematics",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./packages/schematics",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "schematics-e2e": {
      "projectType": "application",
      "root": "e2e\\schematics-e2e",
      "sourceRoot": "e2e\\schematics-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "schematics:build",
            "npmPackageName": "@ngworker/schematics",
            "pluginOutputPath": "dist/packages/schematics",
            "jestConfig": "e2e/schematics-e2e/jest.config.js"
          }
        }
      }
    }
  }
}