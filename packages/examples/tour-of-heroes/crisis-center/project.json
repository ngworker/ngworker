{
  "name": "crisis-center",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/examples/tour-of-heroes/crisis-center/src",
  "prefix": "app-crisis",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": [
        "{workspaceRoot}/dist/packages/examples/tour-of-heroes/crisis-center"
      ],
      "options": {
        "project": "packages/examples/tour-of-heroes/crisis-center/ng-package.json"
      },
      "configurations": {
        "development": {
          "tsConfig": "packages/examples/tour-of-heroes/crisis-center/tsconfig.lib.json"
        },
        "production": {
          "tsConfig": "packages/examples/tour-of-heroes/crisis-center/tsconfig.lib.prod.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/examples/tour-of-heroes/crisis-center/src/**/*.ts",
          "packages/examples/tour-of-heroes/crisis-center/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/packages/examples/tour-of-heroes/crisis-center"
      ],
      "options": {
        "jestConfig": "packages/examples/tour-of-heroes/crisis-center/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:internal", "type:feature"]
}
