{
  "name": "ngworker",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "preinstall": "(pnpm remove codelyzer --silent || echo ✅ Codelyzer is already removed.) && (pnpm remove tslint --silent || echo ✅ TSLint is already removed.)",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "nx serve",
    "build": "nx run-many --target=build --configuration=production --all --with-deps",
    "test": "nx run-many --target=test --all --parallel",
    "lint": "nx workspace-lint && nx run-many --target=lint --all --parallel",
    "e2e": "nx run-many --target=e2e --all --parallel",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build --configuration=production --with-deps",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test --parallel",
    "affected:lint": "nx workspace-lint && nx affected:lint --parallel",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "contributors:add": "all-contributors add",
    "format": "pnpm sort-projects && nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "sort-projects": "json -I -f workspace.json -e \"this.projects = Object.fromEntries(Object.entries(this.projects).sort(([a], [b]) => a.localeCompare(b)));\" && json -I -f nx.json -e \"this.projects = Object.fromEntries(Object.entries(this.projects).sort(([a], [b]) => a.localeCompare(b)));\"",
    "ci": "pnpm lint && pnpm test && pnpm build && pnpm e2e",
    "affected:ci": "pnpm affected:lint && pnpm affected:test && pnpm affected:build && pnpm affected:e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.9",
    "@angular/common": "~11.0.9",
    "@angular/compiler": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/forms": "~11.0.9",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/router": "~11.0.9",
    "@nrwl/angular": "11.2.10",
    "rxjs": "~6.5.5",
    "tslib": "^2.1.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular-eslint/eslint-plugin": "1.0.0",
    "@angular-eslint/eslint-plugin-template": "1.0.0",
    "@angular-eslint/template-parser": "1.0.0",
    "@angular/cli": "~11.0.7",
    "@angular/compiler-cli": "~11.0.9",
    "@angular/language-service": "~11.0.9",
    "@docusaurus/core": "^2.0.0-alpha.70",
    "@docusaurus/module-type-aliases": "^2.0.0-beta.0",
    "@docusaurus/preset-classic": "^2.0.0-alpha.70",
    "@mdx-js/react": "^1.6.21",
    "@nrwl/cli": "11.2.10",
    "@nrwl/cypress": "11.2.10",
    "@nrwl/eslint-plugin-nx": "11.2.10",
    "@nrwl/jest": "11.2.10",
    "@nrwl/node": "11.2.10",
    "@nrwl/nx-cloud": "11.1.3",
    "@nrwl/tao": "11.2.10",
    "@nrwl/workspace": "11.2.10",
    "@nx-plus/docusaurus": "11.0.1",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/jest": "26.0.8",
    "@types/node": "12.12.38",
    "@types/react": "^17.0.9",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "all-contributors-cli": "^6.19.0",
    "clsx": "^1.1.1",
    "cypress": "^6.4.0",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^31.6.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.2",
    "json": "^10.0.0",
    "ng-packagr": "^11.1.4",
    "prettier": "2.2.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "typescript": "~4.0.7",
    "url-loader": "4.1.1"
  },
  "volta": {
    "node": "12.20.1"
  }
}
